{
  "name": "movable-tabs",
  "version": "1.0.0",
  "description": "html5 web component for tabs that can slide using touch events",
  "author": {
    "name": "Tushar Mathur &lt;tusharmath@gmail.com&gt;",
    "url": "http://tusharm.com"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "babel src --out-dir .dist --source-maps true",
    "prepublish": "npm run build",
    "test": "ava",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "doc": "jsdoc2md ./src/** --global-index-format grouped --template README.template.md > README.md",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/movable-tabs.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": ".dist/index.js",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  },
  "keywords": [
    "webcomponent",
    "html5",
    "touch",
    "mobile",
    "tabs",
    "sections"
  ],
  "ava": {
    "require": "babel-register"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-register": "^6.11.6",
    "coveralls": "^2.11.12",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^10.2.1",
    "ghooks": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "node-dev": "^3.1.3",
    "nyc": "^10.3.2",
    "repl.history": "^0.1.3",
    "semantic-release": "^6.3.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "validate-commit-msg": "^2.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "flex-jss": "^1.0.1",
    "hyperscript": "^2.0.2",
    "jss": "^7.1.5",
    "jss-preset-default": "^2.0.0",
    "ramda": "^0.24.0"
  }
}
